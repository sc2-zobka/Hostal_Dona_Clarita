--  DDL for Trigger RECEPCION_ORDEN_PEDIDO_TRG
--------------------------------------------------------

  CREATE OR REPLACE NONEDITIONABLE TRIGGER "PORTAFOLIO"."RECEPCION_ORDEN_PEDIDO_TRG" 
BEFORE INSERT ON RECEPCION_ORDEN_PEDIDO 
FOR EACH ROW 
BEGIN
  <<COLUMN_SEQUENCES>>
  BEGIN
    IF INSERTING AND :NEW.ID_REGISTRO_PEDIDO IS NULL THEN
      SELECT RECEPCION_ORDEN_PEDIDO_SEQ.NEXTVAL INTO :NEW.ID_REGISTRO_PEDIDO FROM SYS.DUAL;
	  
	  IF :NEW.ID_ORDEN_PEDIDO IS NOT NULL THEN
		UPDATE ORDEN_PEDIDO
		SET ID_ESTATUS_ORDEN_PEDIDO = ( SELECT ID_ESTATUS_ORDEN_PEDIDO FROM ESTATUS_ORDEN_PEDIDO WHERE DESCRIPCION LIKE 'RECEPCIONADO' )
		WHERE ID_ORDEN_PEDIDO = :NEW.ID_ORDEN_PEDIDO;
	  END IF;
    END IF;
  END COLUMN_SEQUENCES;
END;
