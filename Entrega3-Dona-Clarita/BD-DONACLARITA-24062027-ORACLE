--------------------------------------------------------
--  DDL for Procedure SP_BUSCARVENTAS
--------------------------------------------------------
set define off;

  CREATE OR REPLACE NONEDITIONABLE PROCEDURE "PORTAFOLIO"."SP_BUSCARVENTAS" ( fechaDesde in varchar2, fechaHasta in varchar2, p_cursor out sys_refcursor )
IS
BEGIN
  -- get contact based on customer id
  open p_cursor for
  SELECT ROW_NUMBER() OVER (ORDER BY TO_CHAR(D.FECHA_EMISION, 'DD-MM-YYYY'))TT ,TD.ABREVIADO, TD.DESCRIPCION AS "TIPO_DOC",D.ID_DOCUMENTO AS "NRO_DOC",
        TO_CHAR(D.FECHA_EMISION, 'DD-MM-YYYY') AS "FEC_EMISION",
        D.ID_ORDEN_DE_COMPRA AS "OC",
        NVL(TO_CHAR(D.FECHA_ANULACION,'DD-MM-YYYY'),'-') AS "FEC_ANULA",
        NVL(TO_CHAR(D.DOC_ANULADO),'-') AS "DOC_ANULA"
    FROM DOCUMENTO D 
    JOIN TIPO_DOCUMENTO TD ON D.CODIGO_SII = TD.CODIGO_SII
    JOIN ESTATUS_DOCUMENTO ED ON D.ID_ESTATUS_DOCUMENTO = ED.ID_ESTATUS_DOCUMENTO
    WHERE TO_CHAR(D.FECHA_EMISION,'YYYY-MM-DD') BETWEEN fechaDesde AND fechaHasta;

  -- print out contact's information
EXCEPTION
   WHEN OTHERS THEN
      dbms_output.put_line( SQLERRM );
END;

/
